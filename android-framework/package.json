{
  "name": "humidity-app-android",
  "version": "1.0.0",
  "description": "Android framework for Humidity App",
  "main": "index.js",
  "scripts": {
    "init": "powershell -ExecutionPolicy Bypass -File scripts/init-capacitor.ps1",
    "build:debug": "npx cap build android --debug",
    "build:release": "npx cap build android --release",
    "sync": "npx cap sync android",
    "copy": "npx cap copy android",
    "open": "npx cap open android",
    "clean": "powershell -ExecutionPolicy Bypass -File scripts/clean-android.ps1"
  },
  "dependencies": {
    "@capacitor/cli": "^5.4.1",
    "@capacitor/core": "^5.4.1",
    "@capacitor/android": "^5.4.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capacitor/network": "^5.0.6",
    "@capacitor/geolocation": "^5.0.6",
    "@capacitor/device": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.4.1"
  }
}
